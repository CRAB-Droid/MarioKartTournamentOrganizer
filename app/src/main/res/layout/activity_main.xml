<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/locationInput"
        android:layout_width="344dp"
        android:layout_height="42dp"
        android:layout_marginStart="33dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="45dp"
        android:text="input"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/participantsHeading"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/locationHeading" />

    <TextView
        android:id="@+id/locationHeading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:layout_marginEnd="261dp"
        android:layout_marginBottom="18dp"
        android:text="Location"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/locationInput"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timeDateInput" />

    <TextView
        android:id="@+id/resultsInput"
        android:layout_width="344dp"
        android:layout_height="42dp"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="67dp"
        android:text="input"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resultsHeading" />

    <TextView
        android:id="@+id/participantsInput"
        android:layout_width="344dp"
        android:layout_height="42dp"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="42dp"
        android:text="input"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/resultsHeading"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/participantsHeading" />

    <TextView
        android:id="@+id/timeDateHeading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:layout_marginEnd="183dp"
        android:layout_marginBottom="18dp"
        android:text="Time and Date"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/timeDateInput"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/heading" />

    <TextView
        android:id="@+id/participantsHeading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="215dp"
        android:layout_marginBottom="16dp"
        android:text="Participants"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/participantsInput"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/locationInput" />

    <TextView
        android:id="@+id/resultsHeading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="277dp"
        android:layout_marginBottom="10dp"
        android:text="Results"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/resultsInput"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/participantsInput" />

    <TextView
        android:id="@+id/timeDateInput"
        android:layout_width="344dp"
        android:layout_height="42dp"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="40dp"
        android:text="input"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/locationHeading"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timeDateHeading" />

    <TextView
        android:id="@+id/heading"
        android:layout_width="370dp"
        android:layout_height="43dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="58dp"
        android:text="Past Mario Kart Tournament"
        android:textSize="29sp"
        app:layout_constraintBottom_toTopOf="@+id/timeDateHeading"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>